{
  "name": "flowApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "flow": "flow",
    "flow-stop": "flow stop",
    "unbundle-base": "react-native ram-bundle --entry-file base.js --platform android --dev false --bundle-output ./android_bundle/base/unbundle/entry.js",
    "unbundle-business": "react-native ram-bundle --entry-file index.js --platform android --dev false --bundle-output ./android_bundle/business/unbundle/entry.js",
    "build:business": "node ./node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file index.js  --bundle-output ./android_bundle/business/bundle/business.android.bundle --sourcemap-output ./android_bundle/business/bundle/business.android.map --assets-dest ./android_bundle/business/bundle/asserts  --config ../../../../business.config.js",
    "build:base": "node ./node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file base.js  --bundle-output ./android_bundle/base/bundle/base.android.bundle --sourcemap-output ./android_bundle/base/bundle/base.android.map --assets-dest ./android_bundle/base/bundle/base/asserts  --config ../../../../metro.config.js",
    "build:android": "node node_modules/react-native/local-cli/cli.js bundle --platform android  --dev true --entry-file index.js  --bundle-output  android/app/src/main/assets/index.android.bundle  --assets-dest  android/app/src/main/res/"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "immutable": "^4.0.0-rc.12",
    "react": "16.6.1",
    "react-native": "0.57.5"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "flow-bin": "0.78.0",
    "jest": "23.6.0",
    "metro": "^0.49.2",
    "metro-core": "^0.49.2",
    "metro-react-native-babel-preset": "0.49.2",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
